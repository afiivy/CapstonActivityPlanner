generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}


generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Questions Array {
  id        Int      @id @default(autoincrement())
  questions  
  choices    String    
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

